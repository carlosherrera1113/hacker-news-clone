{
  "name": "hacker-news-clone",
  "version": "1.0.0",
  "description": "An API clone of the popular news site for computer science using TypeScript, GraphQL, and Node.js",
  "main": "src/index.ts",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc"
  },
  "author": "Carlos Herrera",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.15.1",
    "apollo-server-express": "^2.15.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.7.0",
    "graphql-import": "^0.7.1",
    "jsonwebtoken": "^8.5.1",
    "prisma-client-lib": "^1.34.10"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.17.0",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/introspection": "1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-resolvers": "^1.17.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^12.12.50",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "graphql-code-generator": "^0.18.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
